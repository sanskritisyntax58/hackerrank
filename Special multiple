#include <bits/stdc++.h>
using namespace std;

string solve(int n) {
    queue<string> q;
    q.push("9");

    while (!q.empty()) {
        string s = q.front();
        q.pop();

        long long num = stoll(s);  // convert string to number
        if (num % n == 0) {
            return s;
        }

        q.push(s + "0");
        q.push(s + "9");
    }
    return ""; // should never reach
}

int main() {
    int t; 
    cin >> t;
    while (t--) {
        int n; 
        cin >> n;
        cout << solve(n) << "\n";
    }
    return 0;
}
